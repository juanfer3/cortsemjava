    <queryString language="SQL">
		<![CDATA[SELECT
     clientes.`nombre`,
     clientes.`id`,
     pedidos.`id`,
     clientes.`documento`,
     pedidos.`f_pedido`,
     pedidos.`f_entrega`,
     clientes.`telefono` AS clientes_telefono,
     clientes.`celular` AS clientes_celular,
     clientes.`direccion` AS clientes_direccion,
     clientes.`correo` AS clientes_correo,
     clientes.`cargo` AS clientes_cargo,
     clientes.`usuario_id` AS clientes_usuario_id,
     clientes.`habilitado` AS clientes_habilitado,
     pedidos.`cliente_id` AS pedidos_cliente_id,
     pedidos.`habilitado` AS pedidos_habilitado
FROM
     `clientes` clientes INNER JOIN `pedidos` pedidos ON clientes.`id` = pedidos.`cliente_id`
where

pedidos.id=$P{id};
]]>
    </queryString>
